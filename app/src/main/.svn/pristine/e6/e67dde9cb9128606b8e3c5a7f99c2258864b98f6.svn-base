tent"                android:hint="Address2"/>            <EditText                android:id="@+id/address3"                android:layout_width="fill_parent"                android:layout_height="wrap_content"                android:hint="Address3"/>            <EditText                android:id="@+id/emailId"                android:layout_width="fill_parent"                android:layout_height="wrap_content"                android:hint="Email Id"/>            <EditText                android:id="@+id/password"                android:layout_width="fill_parent"                android:layout_height="wrap_content"                android:hint="Password"/>        </LinearLayout>    </RelativeLayout></LinearLayout>