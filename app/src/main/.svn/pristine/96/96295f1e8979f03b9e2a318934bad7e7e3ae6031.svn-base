<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#130E07">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/scrollincide"
        android:layout_gravity="center_vertical" >

        <RelativeLayout
            android:id="@+id/RelativeLayout01"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <LinearLayout
                android:id="@+id/LinearLayout01"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical" >



                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Vehicle Inspection"
                    android:id="@+id/textView28"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center_horizontal" />

                <DatePicker
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="50dp"
                    android:layout_marginRight="50dp"
                    android:background="#CCCCCC"
                    android:id="@+id/IncidentPicker"
                    android:layout_gravity="center_horizontal" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_marginTop="10dp"
                    android:background="#96CC41"
                    android:text="Vehicle/Tractor"
                    android:textColor="#FFFFFF"
                    android:textAlignment="center"
                    android:textSize="20dp"
                    android:id="@+id/textView36" />

            <LinearLayout
                android:id="@+id/LinearLayout03"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:orientation="vertical" >
                <CheckBox android:id="@+id/steering"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Steering"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/body"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Body"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/tires"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Tires,Wheels,Rims"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/reflect"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Reflectors"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/electric"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Electric Lines to Trailer"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/horn"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Horn"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/clearance"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Clearance Marker Lights"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>

                <CheckBox android:id="@+id/Ammeter"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Ammeter"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/CouplingDevice"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Coupling Device"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/CoolingDevice"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Cooling Device"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/StopTurn"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Stop Turn Lights"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/Transmission"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Transmission"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/Leaks"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Leaks"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/TailLights"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Tail Lights"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>

                <CheckBox android:id="@+id/ServiceBrakes"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Service Brakes"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/Engine"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Engine"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/SuspensionSystem"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Suspension System"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/Glass"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Glass"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/Exhaust"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Exhaust"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/OtherItems"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Other Items"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/ParkingBrakes"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Parking Brakes"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>

                <CheckBox android:id="@+id/DriveLine"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Drive Line"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/FrameAssembly"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Frame and Assembly"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/BrakeTransfer"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Brake Lines to Transfer"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/Clutch"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Clutch"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>

                <CheckBox android:id="@+id/Speedometer"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Speedometer"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/RearVisionMirror"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Rear Vision Mirror"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/WindshieldWipers"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Windshield Wipers"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/OilPressure"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Oil Pressure"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/HeadLights"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Head Lights"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/AirPressureWarning"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Air Pressure Warning Device"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                <CheckBox android:id="@+id/FuelSystem"
                    android:layout_height="25dp"
                    android:layout_width="match_parent"
                    android:text="Fuel System"
                    android:textSize="18dp"
                    android:layout_marginTop="5dp"/>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_marginTop="10dp"
                    android:background="#96CC41"
                    android:text="Trailer"
                    android:textColor="#FFFFFF"
                    android:textAlignment="center"
                    android:textSize="20dp"
                    android:id="@+id/textView37" />

                <LinearLayout
                    android:id="@+id/LinearLayout04"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:orientation="vertical" >
                    <CheckBox android:id="@+id/Suspension"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Suspension System"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/OtherItem"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Other Items"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/CouplingKing"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Coupling King Pin"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/Tarpaulin"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Tarpaulin"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/Roof"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Roof"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/LandingGear"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Landing Gear"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/Hitch"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Hitch"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>

                    <CheckBox android:id="@+id/Doors"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Doors"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/CoupDevice"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Coupling Device"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/Tires"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Tires"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/Brakes"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Brakes"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/LightsAll"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Lights - All"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/BrakeConnection"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Brake Connection"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/WheelsRims"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Wheels - Rims"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_marginTop="10dp"
                    android:background="#96CC41"
                    android:text="Emergency Equipment"
                    android:textColor="#FFFFFF"
                    android:textAlignment="center"
                    android:textSize="20dp"
                    android:id="@+id/textView38" />

                <LinearLayout
                    android:id="@+id/LinearLayout05"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:orientation="vertical" >
                    <CheckBox android:id="@+id/TireChains"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Tire Chains"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/ReflectiveTraingles"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Reflective Traingles"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/SpareFuses"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Spare Bulbs, Fuses"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/Extinguisher"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Fire Extinguisher"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <CheckBox android:id="@+id/FlagsFusees"
                        android:layout_height="25dp"
                        android:layout_width="match_parent"
                        android:text="Flags, Fusees, Reflectors"
                        android:textSize="18dp"
                        android:layout_marginTop="5dp"/>
                    <EditText android:id="@+id/vehicle_comment"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="10dp"
                        android:background="#CCCCCC"
                        android:hint="Comments"
                        style="@style/editext_normal" />
                </LinearLayout>



                <Button android:id="@id/btnincidesubmit"
                    android:layout_width="200dp" android:layout_height="35dp"
                    android:gravity="right"
                    android:layout_gravity="center_horizontal"
                    android:background="#92C83E"
                    android:text="SUBMIT"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="22dp"
                    android:layout_marginTop="30dp" />
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</LinearLayout>
