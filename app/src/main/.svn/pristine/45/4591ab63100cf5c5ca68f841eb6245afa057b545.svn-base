<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/black"
    android:weightSum="4"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp">

    <LinearLayout style="@style/home_row">
        <ImageView style="@style/home_cell" android:src="@drawable/home_status"/>
        <ImageView style="@style/home_cell" android:src="@drawable/home_settings"/>
        <ImageView style="@style/home_cell" android:src="@drawable/home_vehicle"/>
    </LinearLayout>

    <LinearLayout style="@style/home_row">
        <ImageView style="@style/home_cell" android:src="@drawable/home_trip"/>
        <ImageView style="@style/home_cell" android:src="@drawable/home_alert"/>
        <ImageView style="@style/home_cell" android:src="@drawable/home_eobr"/>
    </LinearLayout>

    <LinearLayout style="@style/home_row">
        <ImageView style="@style/home_cell" android:src="@drawable/home_tpms"/>
        <ImageView style="@style/home_cell" android:src="@drawable/home_pol"/>
        <ImageView style="@style/home_cell" android:src="@drawable/home_navigate"/>
    </LinearLayout>

    <LinearLayout style="@style/home_row">
        <ImageView style="@style/home_cell" android:src="@drawable/home_map"/>
        <ImageView style="@style/home_cell" android:src="@drawable/home_fuellog"/>
        <ImageView style="@style/home_cell" android:src="@drawable/home_logout"/>
    </LinearLayout>

</LinearLayout>


