<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="btn" parent="@android:style/Widget.Holo.Light.Button">
        <item name="android:minWidth">1.0dip</item>
        <item name="android:minHeight">1.0dip</item>
    </style>
    <style name="tab_btn" parent="@style/btn">
        <item name="android:textColor">@color/tab_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_tab</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="custom_switch">
        <item name="android:textStyle">bold</item>
        <item name="android:thumb">@drawable/swith_thumb</item>
        <item name="android:track">@drawable/switch_bg</item>
    </style>
    <style name="gray_dk_btn" parent="@android:style/Widget.Holo.Light.Button">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/main_color_gray</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:minWidth">200.0dip</item>
        <item name="android:minHeight">1.0dip</item>
    </style>
    <style name="gray_light_btn" parent="@android:style/Widget.Holo.Light.Button">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/main_color_gray</item>
        <item name="android:background">@color/main_color_gray_light</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:minWidth">10.0dip</item>
        <item name="android:minHeight">1.0dip</item>
        <item name="android:drawablePadding">5.0dip</item>
    </style>

    <style name="editext_normal" parent="@android:style/Widget.EditText">
        <item name="android:textSize">16dp</item>
        <item name="android:ems">10</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:inputType">textEmailAddress</item>
    </style>

    <style name="lbl_explore" parent="@android:style/Widget.Holo.Light.TextView">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@color/main_color_gray_light</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginBottom">5.0dip</item>
    </style>
    <style name="img_explore">
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:layout_marginRight">5.0dip</item>
    </style>
    <style name="lbl_feed" parent="@android:style/Widget.Holo.Light.TextView">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/main_color_gray</item>
        <item name="android:drawablePadding">3dp</item>
    </style>

    <!-- cs styles -->

    <style name="common_btn">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@color/left_nav_press_color</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
    </style>

    <style name="common_round_btn">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/common_button_round_shape</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
    </style>

    <style name="edittext_search" parent="@android:style/Widget.Holo.Light.EditText">
        <item name="android:textSize">15dp</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/search_input_border</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingRight">5.0dip</item>
        <item name="android:layout_marginLeft">20.0dip</item>
        <item name="android:layout_marginTop">15.0dip</item>
        <item name="android:layout_marginRight">20.0dip</item>
        <item name="android:layout_marginBottom">15.0dip</item>
        <item name="android:ems">8</item>
        <item name="android:drawableRight">@drawable/ic_search</item>
        <item name="android:drawablePadding">5.0dip</item>
        <item name="android:textCursorDrawable">@drawable/search_cursor</item>
    </style>

    <style name="common_txt">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/left_nav_press_color</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>

    <style name="tpms_circle_green">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:src">@drawable/ic_tpms_green</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
    </style>



</resources>
